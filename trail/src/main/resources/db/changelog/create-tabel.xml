<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet id="100" author="dinesh">
        <createTable tableName="profile1">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="true"></constraints>
            </column>
            <column name="name" type="varchar(60)">

            </column>
            <column name="email" type="varchar(60)">
                <constraints unique="true"></constraints>
            </column>
            <column name="mobile" type="varchar(10)">

            </column>
            <column name="password" type="varchar(255)"></column>
            <column name="role" type="varchar(50)"></column>

        </createTable>
    </changeSet>
